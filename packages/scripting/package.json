{
  "name": "@formula/scripting",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./node": {
      "types": "./src/node.d.ts",
      "default": "./src/node.js"
    },
    "./web": {
      "types": "./src/web.d.ts",
      "default": "./src/web.js"
    }
  },
  "scripts": {
    "test": "node ../../scripts/run-vitest.mjs -c vitest.config.ts"
  },
  "dependencies": {
    "@formula/security": "workspace:*",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "vitest": "^3.0.7"
  }
}
